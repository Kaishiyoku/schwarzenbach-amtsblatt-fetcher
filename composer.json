{
    "name": "kaishiyoku/schwarzenbach-amtsblatt-fetcher",
    "description": "",
    "keywords": ["framework", "laravel", "lumen"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.3",
        "filp/whoops": "^2.9.0",
        "flipbox/lumen-generator": "^8.0.0",
        "guzzlehttp/guzzle": "^7.2.0",
        "illuminate/mail": "^8.0.0",
        "kaishiyoku/lumen-vendor-publish": "^8.0.0",
        "laravel/lumen-framework": "^8.0.0",
        "league/flysystem": "^1.1.3",
        "spatie/regex": "^1.4.1",
        "symfony/css-selector": "^5.1.7",
        "symfony/dom-crawler": "^5.1.7"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.8.1",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.4.2",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "ide-helper": [
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "php artisan ide-helper:models --write"
        ]
    }
}
